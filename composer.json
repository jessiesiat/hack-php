{
	"name": "jessiesiat/hack-php",
    "type": "project",
    "description": "Small framework for hackers",
    "keywords": ["framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jessie Cris Siat",
            "email": "jcris.siat@gmail.com"
        }
    ],
	"require": {
        "php": ">=5.3.0",
		"pimple/pimple": "3.0.0",
		"symfony/console": "2.6.*",
		"symfony/routing": "2.6.*",
		"symfony/http-kernel": "2.6.*",
		"twig/twig": "~1.0",
		"monolog/monolog": "1.13.*",
		"illuminate/support": "5.0.*"
	},
	"require-dev": {
        "phpunit/phpunit": "~4.0"
    },
	"autoload": {
		"psr-4": {
			"Hack\\": "src/"
		},
		"files": [
            "src/helpers.php"
        ]
	},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
